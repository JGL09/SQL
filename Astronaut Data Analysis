/* Found the Astronaut with the max number of space flights */
SELECT Name, MAX(Space_Flights) AS "Most Flights From Sample" 
    FROM Astronauts;

/* Found the Astronauts with more than one Flight to Space */    
SELECT Name, Space_Flights FROM astronauts
    GROUP BY Space_Flights
    HAVING Space_Flights > 1;

/* Used the CASE method to classify each astronaut into a category based on the recency of their visit to space */
SELECT count(*) AS 'Number of Astronauts',
    CASE
        WHEN Year > 1990 THEN 'Recent'
        WHEN Year > 1980 THEN 'Near Recent'
        When Year > 1970 THEN 'Old'
        WHEN Year > 1960 THEN 'Ancient'
        ELSE 'Test Dummies'
    END AS 'Recency_of_Flights' FROM astronauts
    Group BY Recency_of_Flights ORDER BY count(*);

/* Used CASE method to classify each pilot into Male or Female and then find out the total astronauts in each gender */
SELECT count(*) AS "Number of Astronauts",
    CASE
        WHEN Gender = 'Male' THEN 'Male'
        WHEN Gender = 'Female' THEN 'Female'
        ELSE 'NULL'
    END AS 'GENDER' FROM astronauts
    GROUP BY Gender;

/* Used the CASE method and AND to classify each pilot into a rank based on their space flight hours */
SELECT count(*) AS 'Number of Astronauts per Rank',
    CASE
        WHEN Space_Flight_hr > 0 AND Space_Flight_hr < 200 THEN 'Newbie'
        WHEN Space_Flight_hr > 200 AND Space_Flight_hr < 500 THEN 'Beginner'
        WHEN Space_FLight_hr > 500 AND Space_Flight_hr < 1000 THEN 'Intermediate'
        WHEN Space_Flight_hr > 1000 AND Space_Flight_hr < 2500 THEN 'Pro'
        WHEN Space_Flight_hr > 2500 THEN 'Legend'
        ELSE 'No Flights'
    END AS 'Space_Flight_Ranks' FROM astronauts
    GROUP BY Space_Flight_Ranks
    ORDER BY count(*);

/* Used the HAVING method to find astronauts that have more than 500 hours of flights time and are also Female */
SELECT Name AS "Female Pilot With 500+ hr's", Space_Flight_hr FROM astronauts
    GROUP BY Space_Flight_hr
    HAVING 
        Space_Flight_hr > 500
        AND
        Gender = 'Female';
